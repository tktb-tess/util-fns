{
  "name": "@tktb-tess/util-fns",
  "version": "0.3.4",
  "description": "Utility functions for personal use",
  "files": [
    "dist"
  ],
  "type": "module",
  "author": "Tessyrrhaqt",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/bundle.js",
  "types": "./dist/bundle.d.ts",
  "exports": {
    "types": "./dist/bundle.d.ts",
    "import": "./dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tktb-tess/util-fns"
  },
  "homepage": "https://github.com/tktb-tess/util-fns/blob/main/README.md",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "update": "ncu -u && npm i",
    "wasm:clean": "rm -r -f lib/wasm",
    "wasm:build": "npm run wasm:clean && cd wasm && wasm-pack build --no-pack --out-dir ../lib/wasm"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "jsdom": "^26.1.0",
    "npm-check-updates": "^18.0.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  }
}
