{
  "name": "@tktb-tess/util-fns",
  "version": "0.4.0",
  "description": "Utility functions for personal use",
  "files": [
    "dist"
  ],
  "type": "module",
  "author": "Tessyrrhaqt",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/bundle.umd.cjs",
  "module": "./dist/bundle.js",
  "types": "./dist/bundle.d.ts",
  "exports": {
    "types": "./dist/bundle.d.ts",
    "require": "./dist/bundle.umd.cjs",
    "import": "./dist/bundle.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tktb-tess/util-fns"
  },
  "homepage": "https://github.com/tktb-tess/util-fns/blob/main/README.md",
  "scripts": {
    "dev": "vite",
    "build:vite": "tsc -p tsconfig.lib.json && vite build",
    "test": "vitest run",
    "update": "ncu -u && npm i",
    "clean-wasm": "rm -r -f lib/wasm",
    "build:wasm": "rm -r -f lib/wasm && cd wasm-part && wasm-pack build --no-pack --out-dir ../lib/wasm --target web",
    "build": "npm run build:wasm && npm run build:vite"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.3.0",
    "jsdom": "^26.1.0",
    "npm-check-updates": "^18.0.3",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "22.19.0"
  }
}
